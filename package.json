{
  "name": "aq-html-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "ie 11",
    "iOS 10",
    "Android 4"
  ],
  "scripts": {
    "dev:sass": "gulp sass",
    "dev:webpack": "webpack --mode development --watch --config ./config/webpack.config.js",
    "dev:server": "browser-sync start --config ./config/bs-config.js",
    "dev:watch": "gulp watch",
    "dev:watch--amp": "gulp watch--amp",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:server dev:watch dev:webpack",
    "dev--amp": "cross-env NODE_ENV=development npm-run-all --parallel dev:server dev:watch--amp dev:webpack",
    "copy": "gulp copy",
    "crean-all": "gulp crean-all",
    "crean-dev": "gulp crean-dev",
    "build:sass": "gulp sass",
    "build:postcss": "postcss src/**/*.css --no-map --dir htdocs --base src --config ./config/postcss.config.js",
    "build:purgecss": "gulp purgecss",
    "build:check-ampcss": "gulp check-ampcss",
    "build:inject-amphtml": "gulp inject-amphtml",
    "build:webpack": "webpack --mode production --config ./config/webpack.config.js",
    "build:pug": "gulp pug",
    "build:imagemin": "gulp imagemin",
    "build": "cross-env NODE_ENV=production npm-run-all crean-all copy build:pug build:sass build:purgecss build:postcss build:webpack build:imagemin crean-dev",
    "build--amp": "cross-env NODE_ENV=production npm-run-all crean-all copy build:pug build:sass build:purgecss build:postcss build:check-ampcss build:inject-amphtml build:webpack build:imagemin crean-dev",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "core-js": "3",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "del": "^4.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gulp-data": "^1.3.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "jest": "^24.7.1",
    "jquery": "3.5.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-cli": "^6.1.2",
    "pug": "^2.0.3",
    "pug-cli": "github:pugjs/pug-cli#master",
    "scrolltrigger-classes": "^0.3.6",
    "stylus": "^0.54.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@babel/register": "^7.4.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-check-filesize": "^2.0.1",
    "gulp-inject-string": "^1.1.2",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-purgecss": "^2.0.6",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "5.0.1",
    "node-sass": "^4.13.1",
    "object-fit-images": "^3.2.4",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10"
  }
}
