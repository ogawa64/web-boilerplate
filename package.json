{
  "name": "aq-html-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "ie 11",
    "iOS 10",
    "Android 4"
  ],
  "scripts": {
    "dev:sass": "node-sass --source-map-embed --watch src/common/css/scss -r -o src/common/css/",
    "dev:webpack": "webpack --mode development --watch --config ./config/webpack.config.js",
    "dev:server": "browser-sync start --config ./config/bs-config.js",
    "dev:watch": "gulp watch",
    "dev": "NODE_ENV=development npm-run-all --parallel dev:*",
    "copy": "gulp copy",
    "crean-all": "gulp crean-all",
    "crean-dev": "gulp crean-dev",
    "build:sass": "node-sass src/common/css/scss -r -o httpdocs/common/css/",
    "build:postcss": "postcss src/**/*.css --no-map --dir httpdocs --base src --config ./config/postcss.config.js",
    "build:webpack": "webpack --mode production --config ./config/webpack.config.js",
    "build:nunjucks": "gulp nunjucks",
    "build": "NODE_ENV=production npm-run-all copy build:nunjucks build:sass build:webpack crean-dev"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "core-js": "3",
    "cssnano": "^4.1.10",
    "del": "^4.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gulp-data": "^1.3.1",
    "gulp-ejs": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-cli": "^6.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@babel/register": "^7.4.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-nunjucks-render": "^2.2.3",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "node-sass": "^4.11.0"
  }
}
